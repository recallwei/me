{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "skipLibCheck": true,
    "strict": true,
    // "verbatimModuleSyntax": true, Currently use TS 4.9.5 for Astro plugin, does not support this option.
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    }
  }
}
